javascript:var elements=["javascript:"];var allt = document.getElementsByTagName("*");Array.prototype.forEach.call(allt, function(all) { if (all.hasAttribute('id')){ if (all.disabled == true)elements.push("document.getElementById('"+all.id+"').disabled=false;");all.disabled = false;if(all.hasAttribute('readOnly')){elements.push("document.getElementById('"+all.id+"').removeAttribute('readOnly');");all.removeAttribute('readonly');}if (all.hasAttribute('maxlength')){elements.push("document.getElementById('"+all.id+"').removeAttribute('maxlength');");all.removeAttribute('maxlength');};if (all.getAttribute("type")=="hidden"){all.setAttribute("type","text");}; } if (all.hasAttribute('name')){ if (all.disabled == true)elements.push("document.getElementsByName('"+all.name+"')[0].disabled=false;");all.disabled = false;if(all.hasAttribute('readOnly')){elements.push("document.getElementsByName('"+all.name+"')[0].removeAttribute('readOnly');");all.removeAttribute('readonly');}if (all.hasAttribute('maxlength')){elements.push("document.getElementsByName('"+all.name+"')[0].removeAttribute('maxlength');");all.removeAttribute('maxlength');};if (all.getAttribute("type")=="hidden"){all.setAttribute("type","text");}; } }); prompt("All Enabled!\nPress CTRL+C to get the script", elements.join(''));
